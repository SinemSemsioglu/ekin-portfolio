{
  "env": {
    "node": false,
    "browser": true
  },
  //sorted as on wiki
  "rules": {
    //Possible Errors
    "no-reserved-keys": 2,
    "valid-jsdoc": [2, {
      "requireReturn": false
    }],
    //Best Practices
    "block-scoped-var": 2,
    "complexity": [2, 6],
    "default-case": 2,
    "guard-for-in": 2,
    "no-self-compare": 2,
    "no-void": 2,
    "radix": 2,
    "wrap-iife": [2, "inside"],
    //Strict Mode - no item
    //Variables
    "camelcase": [2, {"properties": "never"}],
    //Stylistic Issues
    "brace-style": 2,
    "comma-style": [2, "last"],
    "func-style": [2, "declaration"],
    "max-nested-callbacks": [2, 2],
    "no-multiple-empty-lines": [2, {
      "max": 1
    }],
    //Legacy
    "max-depth": [1, 3],
    "max-len": [2, 120, 4],
    "max-params": [1, 7],
    "no-bitwise": 2,
    "no-space-before-semi": 2,
    "quotes": [2, "double"],
    "space-after-function-name": [2, "never"],
    "space-after-keywords": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-in-brackets": [2, "never"],
    "space-in-parens": [2, "never"],
    "space-unary-ops": [2, {
      "words": true,
      "nonwords": false
    }]
  },
  "globals": {
    "Cookies": true,
    "$": true,
    "angular": true,
    "moment": true
  }
}